                <td class="left">                    
                    <apex:outputField value="{!Application__c.Company_Address_State__c}" id="one-f" />
                </td>
                </tr>

                <tr>
                <td class="right">
                    Country
                </td>
                <td class="left">                    
                    <apex:outputField value="{!Application__c.Company_Address_Country__c}" id="one-g" />
                </td>
                </tr>

                <tr>
