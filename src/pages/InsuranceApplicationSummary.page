                <apex:outputLink value="/apex/InsuranceApplicationSummaryPDF?id={!thisApplication.id}" target="_blank"  styleClass="btn" style="text-decoration:none;padding: 4px;" >Print</apex:outputLink>                
                               
            </div>
            </div>
            </div>   
            <apex:outputPanel id="summarySections">
                           
            <apex:outputPanel >            
            
            <div style="padding-top:50px">
            <div>
                <apex:pageMessages id="showmsg" escape="false"></apex:pageMessages>
            </div>
            <div><h4>{!$Label.Insurance_Summary}</h4></div><br/>
