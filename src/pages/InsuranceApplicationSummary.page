                <td class="right">
                    Is Parent Company Publicly Traded?
                </td>
                <td class="left">
                    <apex:outputField value="{!Application__c.Is_Parent_Company_Publicly_Traded__c}" id="one-p" />
                </td>
                </tr>
                
                <apex:outputPanel layout="none" rendered="{!Application__c.Is_Parent_Company_Publicly_Traded__c='Yes'}">
                <tr>
                <td class="right">
                    Ticker Symbol
                </td>
                <td class="left">
                    <apex:outputField value="{!Application__c.Parent_Company_Ticker_Symbol__c}" id="one-q" />
                </td>
                </tr>             
                </apex:outputPanel>
                </apex:outputPanel>                                   
                </table>
                
                <br />
