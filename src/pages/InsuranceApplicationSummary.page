<apex:page standardController="Application__c" sidebar="false" showHeader="false" extensions="InsuranceApplicationExt" action="{!getAllData}" title="RPXIS Insurance Application">
<apex:stylesheet value="{!URLFOR($Resource.kube)}"/>
    <style>
     
.crumbs { margin-left:auto; margin-right:auto; position:relative; }
.crumbs li a{
    display: block;
    float: left;
    font-size: 11px;
    margin-left: -13px;
    padding: 5px 17px 8px 15px;
    position: relative;
    text-decoration: none;
    font-weight:bold;
}
.crumbs li a:hover {
    color: white;
}

.selected{
    color: white;
    background-image: url('{!$Resource.bgcrumbs}');
    background-repeat: no-repeat;
    width:134px;
    background-position: 100% -96px;
}

.otherSteps{
    color: white;
    background-image: url('{!$Resource.bgcrumbs}');
    background-repeat: no-repeat;
    background-position: 100% -48px;
    width:134px;
    cursor: pointer;
}
.section-header{
margin-bottom: 15px;
padding-left: 170px;
font-weight: bold;
border-bottom: 1px solid #EEE;
padding: 1.5em 0px 0.7em;
font-size: 1.1em;
width: 700px;
text-align: left;
margin-left: -100px;
padding-left: 170px;
}

.left{text-align:left;width:60%;border-bottom:none;}
.right{text-align:right;width:40%;border-bottom:none;font-weight:bold;font-size: 13px;}

.customTable{
    font-weight:bold;
}

.customDivContainer{
    float:left;padding-left:20px;

    //width:120px;
}

.message.messageCell{
    borer-bottom:0px;
}

    </style>
    <apex:form styleClass="forms columnar" style="width:1000px; margin:0 auto" id="theForm">
    <apex:inputHidden value="{!Application__c.Status__c}"/>
    <apex:inputHidden value="{!Application__c.Short_Form__c}"/>        
    <div style="display:inline-block;width:100%;">

    <div>
        <apex:image value="{!$Resource.RPXLogo}" style="float:left; margin-bottom:20px;" />
    </div>
    <div>
        <apex:outputLink value="/apex/InsuranceApplicationStartForm" style="float:right;padding-top:30px;">{!$Label.Home}</apex:outputLink>
    </div>

    </div>
    
    <fieldset>

            <div style="text-align:center;">
                <h5>RPXIS Insurance Application</h5>

