<apex:component controller="MS_RelatedLitigationCtrl" layout="none" id="cmp">
	<apex:form id="frm">

		<apex:pageMessages />

		<apex:sectionHeader title="{!IF(Acq != 0, "Acquisition Overview", IF(Lit != 0, "Related Litigations (by in-suit patents)", "???"))}" subtitle="{!IF(Acq != 0, Acq.Name, IF(Lit != 0, Lit.Name, "???"))}" />

		<c:MS_LockComponent />

		<apex:pageBlock id="Defendants" tabStyle="Account">

			<apex:pageBlockSection title="Defendant Count" columns="2" id="DefCnt">
				<apex:outputText value="{!N_Defendants}" label="Defendants" />
				<apex:outputText value="{!N_Members}" label="Members" />
				<apex:outputText value="{!N_Defendants_Active}" label="Defendants (Active)" />
				<apex:outputText value="{!N_Members_Active}" label="Members (Active)" />
				<apex:outputText value="{!N_Defendants_Terminated}" label="Defendants (Terminated)" />
				<apex:outputText value="{!N_Members_Terminated}" label="Members (Terminated)" />
				<apex:outputText value="" label="" />
				<apex:outputText value="{!N_Prospects}" label="Prospects" />
				<apex:outputText value="" label="" />
				<apex:outputText value="{!N_Prospects_Active}" label="Prospects (Active)" />
				<apex:outputText value="" label="" />
				<apex:outputText value="{!N_Prospects_Terminated}" label="Prospects (Terminated)" />

<!--
				<apex:outputText value="{!xxxS}" label="Plaintiff" />
-->

			</apex:pageBlockSection>

			<apex:pageBlockSection title="Filter" columns="2" id="DefFltr">

				<apex:selectCheckboxes value="{!AccType}" label="Defendant Type" layout="pageDirection" >
					<apex:selectOptions value="{!AccTypeItems}"/>
					<apex:actionsupport event="onchange" action="{!QueryAccounts}" status="MS_LockScreen" rerender="Def" />
				</apex:selectCheckboxes>

				<apex:selectRadio value="{!AccStatus}" label="Defendant Status" layout="pageDirection" >
					<apex:selectOptions value="{!AccStatusItems}"/>
					<apex:actionsupport event="onchange" action="{!QueryAccounts}" status="MS_LockScreen" rerender="Def" />
				</apex:selectRadio>

			</apex:pageBlockSection>

			<apex:pageBlockSection title="Defendants (members in bold)" columns="1" id="Def">
				<apex:pageMessage summary="No records found" severity="warning" strength="1" rendered="{!lstAccounts.Size = 0}"/>
				<apex:pageBlockTable value="{!lstAccounts}" var="Acc" id="DefTbl" rendered="{!lstAccounts.Size > 0}">

					<apex:column headervalue="{!$ObjectType.Account.Fields.Name.Label}" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:commandLink action="{!SortAccounts}" value="{!$ObjectType.Account.Fields.Name.Label}" rerender="Defendants" status="MS_LockScreen">
      	          <apex:param value="Name" name="sort" assignTo="{!Acc_CurrentSortColumn}"/>   
        	      </apex:commandLink>
         	      <img src="{!IF(Acc_CurrentSortColumn = 'Name', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:outputLink value="/{!Acc.acc.Id}" title="View Account" target="_blank">
							<apex:outputField value="{!Acc.acc.Name}" />
						</apex:outputLink>
					</apex:column>

					<apex:column headerValue="RPX" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:outputText value="RPX" />
         	      <img src="/img/s.gif" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:outputLink value="{!'https://portal.rpxcorp.com/entities/' & TEXT(Acc.acc.Entity_ID__c) & '?include=subsidiaries'}" title="View Account on Portal" target="_blank">
							<apex:image value="{!URLFOR($Resource.RPXLogos, 'favicon.jpg')}" />
						</apex:outputLink>
					</apex:column>

					<apex:column headervalue="{!$ObjectType.Account.Fields.Type.Label}" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:commandLink action="{!SortAccounts}" value="{!$ObjectType.Account.Fields.Type.Label}" rerender="Defendants" status="MS_LockScreen">
      	          <apex:param value="Type" name="sort" assignTo="{!Acc_CurrentSortColumn}"/>   
        	      </apex:commandLink>
         	      <img src="{!IF(Acc_CurrentSortColumn = 'Type', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:outputField value="{!Acc.acc.Type}" />
					</apex:column>

					<apex:column headervalue="{!$ObjectType.Account.Fields.RPX_RCRate__c.Label}" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:commandLink action="{!SortAccounts}" value="{!$ObjectType.Account.Fields.RPX_RCRate__c.Label}" rerender="Defendants" status="MS_LockScreen">
      	          <apex:param value="RPX_RCRate__c" name="sort" assignTo="{!Acc_CurrentSortColumn}"/>   
        	      </apex:commandLink>
         	      <img src="{!IF(Acc_CurrentSortColumn = 'RPX_RCRate__c', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:outputField value="{!Acc.acc.RPX_RCRate__c}" />
					</apex:column>

<!--
					<apex:column headervalue="M?" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:commandLink action="{!SortAccounts}" value="M?" title="Account has Membership opportunity?" rerender="Defendants" status="MS_LockScreen">
      	          <apex:param value="XX_bMembershipOpportunity" name="sort" assignTo="{!Acc_CurrentSortColumn}"/>   
        	      </apex:commandLink>
         	      <img src="{!IF(Acc_CurrentSortColumn = 'XX_bMembershipOpportunity', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:image value="{!If(Acc.bMembershipOpportunity, "/img/checkbox_checked.gif", "/img/checkbox_unchecked.gif")}" title="{!If(Acc.bMembershipOpportunity, "Membership Opportunity" , "No Membership Opportunity")}" />
					</apex:column>
-->

