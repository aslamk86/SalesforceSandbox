      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:outputField value="{!Acc.acc.HeatMapColorDisplay__c}" />
					</apex:column>

					<apex:column headervalue="{!$ObjectType.Account.Fields.OwnerId.Label}" style="{!IF(Acc.isMember,"font-weight:bold","")}">
	          <apex:facet name="header">
