								</apex:commandLink>
								<img src="{!IF(Acc_CurrentSortColumn = 'XX_bInsuranceOpportunity', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
							</apex:outputPanel>  
						</apex:facet>
