					<apex:column headervalue="Active?">
	          <apex:facet name="header">
  	          <apex:outputPanel >
    	          <apex:commandLink action="{!SortAccounts}" value="Active?" rerender="Defendants" status="MS_LockScreen">
      	          <apex:param value="XX_isActive" name="sort" assignTo="{!Acc_CurrentSortColumn}"/>   
        	      </apex:commandLink>
         	      <img src="{!IF(Acc_CurrentSortColumn = 'XX_isActive', IF(Acc_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
      	      </apex:outputPanel>  
       	    </apex:facet>
						<apex:image value="{!If(Acc.isActive, "/img/samples/color_green.gif", "/img/samples/color_red.gif")}" height="15" width="30" title="{!If(Acc.isActive, "Active Defendant" , "Inactive Defendant")}" />
					</apex:column>

				</apex:pageBlockTable>
			</apex:pageBlockSection>
		</apex:pageBlock>

		<apex:pageBlock id="Litigations" tabStyle="Litigation__c" rendered="{!(lstRelatedLits.size > 0)}">

			<apex:pageBlockSection title="Litigation Count (for selected litigations)" columns="2" id="LitCnt">
				<apex:outputText value="{!N_Lit}" label="Litigations" />
				<apex:outputText value="{!N_Lit_NPE}" label="NPE" />
				<apex:outputText value="{!N_Lit_Active}" label="Active Litigations" />
				<apex:outputText value="{!N_Lit-N_Lit_NPE}" label="Non-NPE" />
				<apex:outputText value="{!N_Lit_Closed}" label="Closed Litigations" />
				<apex:outputText value="{!N_Lit_DJ}" label="DJs" />
			</apex:pageBlockSection>


			<apex:pageBlockSection title="Related Litigations (current litigation(s) in bold)" columns="1" id="RelLit">
				<apex:commandButton value="{!IF(LitMoreColumns=true, 'Less Detail', 'More Detail')}" action="{!ToggleLitMoreColumns}" status="MS_LockScreen" rerender="RelLit" />
				<apex:pageBlockTable value="{!lstRelatedLits}" var="RelLit" id="RelLitTbl">

					<apex:column >
						<apex:facet name="header">
							<apex:inputCheckBox id="chkSelectAll">
								<apex:actionsupport event="onchange" action="{!UpdateFields}" onsubmit="checkAll(this.checked);" oncomplete="updateCheckAll();" status="MS_LockScreen" rerender="Defendants,Litigations,Acquisitions,Patents" />
							</apex:inputCheckbox>
						</apex:facet>
						<apex:inputCheckBox value="{!RelLit.isSelected}" id="chkList" disabled="{!RelLit.isDisabled}">
							<apex:actionsupport event="onchange" action="{!UpdateFields}" oncomplete="updateCheckAll();" status="MS_LockScreen" rerender="Defendants,Litigations,Acquisitions,Patents" />
						</apex:inputCheckbox>
					</apex:column>

					<apex:column headerValue="{!$ObjectType.Litigation__c.Fields.Name.Label}" style="{!IF(RelLit.isDisabled, "font-weight:bold","")}">
						<apex:facet name="header">
							<apex:outputPanel >
								<apex:commandLink action="{!SortLitigations}" value="{!$ObjectType.Litigation__c.Fields.Name.Label}" rerender="Litigations" status="MS_LockScreen">
									<apex:param value="Name" name="sort" assignTo="{!Lit_CurrentSortColumn}" />
								</apex:commandLink>
								<img src="{!IF(Lit_CurrentSortColumn = 'Name', IF(Lit_IsAsc = true,'/img/sort_asc_arrow.gif','/img/sort_desc_arrow.gif'),'/img/s.gif')}" width="21" height="14" />
							</apex:outputPanel>
						</apex:facet>
						<apex:outputLink value="/{!RelLit.Lit.Id}" title="View Litigation" target="_blank">
							<apex:outputField value="{!RelLit.Lit.Name}" />
						</apex:outputLink>
					</apex:column>

					<apex:column headerValue="RPX" style="{!IF(RelLit.isDisabled, "font-weight:bold","")}">
